{
  "name": "pokedex-typed",
  "version": "1.0.0",
  "description": "A pokedex using the Pok√©API with built-in typings",
  "main": "./src/index.js",
  "module": "./src/index.mjs",
  "types": "./typings/index.d.ts",
  "files": [
    "src",
    "typings"
  ],
  "exports": {
    "./*": "./*",
    ".": {
      "require": "./src/index.js",
      "import": "./src/index.mjs"
    }
  },
  "scripts": {
    "test": "npm run lint && npm run docs:test && npm run lint:typings",
    "test:ts": "tsc",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "lint:typings": "tslint typings/index.d.ts",
    "prettier": "prettier --write src/**/*.js typings/**/*.ts",
    "prepublishOnly": "gen-esm-wrapper ./src/index.js ./src/index.mjs",
    "prepare": "is-ci || husky install",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JTTechnic/pokedex-typed.git"
  },
  "keywords": [],
  "author": "Jaron Zijlstra",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JTTechnic/pokedex-typed/issues"
  },
  "homepage": "https://github.com/JTTechnic/pokedex-typed#readme",
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-angular": "^13.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "dtslint": "^4.1.6",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gen-esm-wrapper": "^1.1.3",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@discordjs/collection": "^0.2.1",
    "node-fetch": "^3.1.1"
  }
}
